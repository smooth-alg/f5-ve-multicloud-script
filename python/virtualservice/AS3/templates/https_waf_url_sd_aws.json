{ 
	"remark":"ASM_VS1",
	"schemaVersion":"3.0.0",
	"label":"ASM_VS1",
	"class":"ADC",
	"tenant":{ 
		"https":{ 
			"WAFPolicy":{ 
				"ignoreChanges":true,
          		"url":"https://raw.githubusercontent.com/f5devcentral/f5-asm-policy-templates/master/owasp_ready_template/owasp-no-auto-tune-v1.1.xml",
				"class":"WAF_Policy"
			},
			"pool":{ 
				"class":"Pool",
				"members":[ 
					{ 
						"servicePort":80,
						"region":"us-west-2",
	                    "tagKey": "aws:autoscaling:groupName",
                        "tagValue": "f5demoapp-prod-v001",
						"addressDiscovery":"aws",
						"updateInterval":10,
						"addressRealm":"private"
					}
				],
				"monitors":[ 
					"http"
				]
			},
			"class":"Application",
			"template":"https",
			"serviceMain":{ 
				"virtualAddresses":[ 
					"0.0.0.0"
				],
				"snat":"auto",
				"policyWAF":{ 
					"use":"WAFPolicy"
				},
				"pool":"pool",
				"serverTLS":{ 
					"bigip":"/Common/clientssl"
				},
				"redirect80":false,
				"class":"Service_HTTPS"
			}
		},
		"class":"Tenant"
	}
}

